    <script>
        document.querySelectorAll('.close-btn').forEach(button => {
            button.addEventListener('click', function(event) {
                event.preventDefault();
                let productId = this.dataset.productId;
    
                fetch(`/removeFromCart?productId=${productId}`, {
                    method: 'GET',
                    credentials: 'same-origin'
                })
                .then(response => {
                    if (response.ok) {
                        window.location.reload();
                    } else {
                        console.error('Failed to remove item from cart');
                    }
                })
                .catch(error => console.error('Error:', error));
            });
        });
    
        document.querySelectorAll('.increase-quantity').forEach(button => {
            button.addEventListener('click', function() {
                let productId = this.dataset.productId;
                let quantityElement = document.getElementById(`quantity${productId}`);
                let totalElement = document.getElementById(`total${productId}`);
                let discountPrice = parseFloat(this.dataset.discountPrice);
                let quantity = parseInt(quantityElement.innerText);
    
                if (quantity < parseInt(this.dataset.inStock)) {
                    quantity++;
                    quantityElement.innerText = quantity;
                    totalElement.innerText = `â‚¹ ${(quantity * discountPrice).toFixed(2)}`;
                    updateCartSummary();
                }
            });
        });
    
        document.querySelectorAll('.decrease-quantity').forEach(button => {
            button.addEventListener('click', function() {
                let productId = this.dataset.productId;
                let quantityElement = document.getElementById(`quantity${productId}`);
                let totalElement = document.getElementById(`total${productId}`);
                let discountPrice = parseFloat(this.dataset.discountPrice);
                let quantity = parseInt(quantityElement.innerText);
    
                if (quantity > 1) {
                    quantity--;
                    quantityElement.innerText = quantity;
                    totalElement.innerText = `â‚¹ ${(quantity * discountPrice).toFixed(2)}`;
                    updateCartSummary();
                }
            });
        });
    
        function updateCartSummary() {
            let totalMRP = 0;
            let totalDiscount = 0;
            let totalPrice = 0;
            let shipping = 0;
            let productCount = 0;
    
            document.querySelectorAll('.cart-item').forEach(cartItem => {
                let originalPrice = parseFloat(cartItem.dataset.originalPrice);
                let discountPrice = parseFloat(cartItem.dataset.discountPrice);
                let quantity = parseInt(cartItem.querySelector('.quantity').innerText);
    
                totalMRP += originalPrice * quantity;
                totalPrice += discountPrice * quantity;
                totalDiscount += (originalPrice - discountPrice) * quantity;
                productCount += quantity;
            });
    
            shipping = productCount > 2 ? 0 : 40;
    
            document.querySelector('.total-mrp').innerText = `â‚¹${totalMRP.toFixed(2)}`;
            document.querySelector('.total-discount').innerText = `â‚¹${totalDiscount > 0 ? totalDiscount.toFixed(2) : 0}`;
            document.querySelector('.total-price').innerText = `â‚¹${totalPrice.toFixed(2)}`;
            document.querySelector('.shipping').innerText = `â‚¹${shipping.toFixed(2)}`;
            document.querySelector('.total').innerText = `â‚¹${(totalPrice + shipping).toFixed(2)}`;
        }
    
        document.querySelectorAll('.increase-quantity, .decrease-quantity').forEach(button => {
            button.addEventListener('click', function() {
                updateCartSummary();
            });
        });
    
        updateCartSummary();
    </script>
