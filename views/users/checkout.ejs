<script>
document.addEventListener('DOMContentLoaded', function() {
    function updateCartQuantity(productId, newQuantity) {
        fetch(`/cart/updateCartItemQuantity/${productId}`, {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ quantity: newQuantity })
        })
        .then(response => response.json())
        .then(data => {
            if (data.success) {
                document.getElementById(`quantity${productId}`).textContent = data.updatedCart.quantity;
                document.getElementById(`total${productId}`).textContent = `â‚¹ ${data.updatedCart.discountPrice * data.updatedCart.quantity}`;
                updateCartSummary(data.cartSummary);
            } else {
                alert(data.message || 'Error updating cart.');
            }
        })
        .catch(error => {
            console.error('Error:', error);
            alert('An error occurred. Please try again.');
        });
    }

    function updateCartSummary(summary) {
        document.querySelector('.total-mrp').textContent = `â‚¹ ${summary.totalMRP}`;
        document.querySelector('.total-discount').textContent = `â‚¹ ${summary.totalDiscount}`;
        document.querySelector('.total-amount').textContent = `â‚¹ ${summary.totalAmount}`;
    }

    document.querySelectorAll('.increase-quantity').forEach(button => {
        button.addEventListener('click', () => {
            const productId = button.dataset.productId;
            const inStock = parseInt(button.dataset.inStock, 10);
            const currentQuantity = parseInt(document.getElementById(`quantity${productId}`).textContent, 10);
            if (currentQuantity < inStock) {
                updateCartQuantity(productId, currentQuantity + 1);
            } else {
                alert('Quantity exceeds stock limit.');
            }
        });
    });

    document.querySelectorAll('.decrease-quantity').forEach(button => {
        button.addEventListener('click', () => {
            const productId = button.dataset.productId;
            const currentQuantity = parseInt(document.getElementById(`quantity${productId}`).textContent, 10);
            if (currentQuantity > 1) {
                updateCartQuantity(productId, currentQuantity - 1);
            } else {
                alert('Quantity cannot be less than 1.');
            }
        });
    });

    document.querySelectorAll('.close-btn').forEach(button => {
        button.addEventListener('click', () => {
            const productId = button.dataset.productId;
            fetch(`/cart/deleteCartItem/${productId}`, {
                method: 'POST'
            })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    document.querySelector(`.cart-item[data-product-id="${productId}"]`).remove();
                    updateCartSummary(data.cartSummary);
                    if (data.cartSummary.totalMRP === 0) {
                        location.reload();
                    }
                } else {
                    alert(data.message || 'Error removing item from cart.');
                }
            })
            .catch(error => {
                console.error('Error:', error);
                alert('An error occurred. Please try again.');
            });
        });
    });

    document.getElementById('checkout-button').addEventListener('click', () => {
        fetch('/order/createOrder', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ /* pass necessary order data */ })
        })
        .then(response => response.json())
        .then(data => {
            if (data.success) {
                window.location.href = '/checkout';
            } else {
                alert(data.message || 'Error proceeding to checkout.');
            }
        })
        .catch(error => {
            console.error('Error:', error);
            alert('An error occurred. Please try again.');
        });
    });
});
</script>

